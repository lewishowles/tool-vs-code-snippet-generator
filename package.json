{
	"name": "vscode-snippet-generator",
	"version": "0.0.1",
	"description": "A tool to help quickly turn code into re-usable VSCode snippets.",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test:unit": "vitest",
		"test:unit:run": "vitest run",
		"test:component": "cypress run --component",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"lint": "eslint . --fix",
		"deploy": "vite build; gh-pages -d dist"
	},
	"dependencies": {
		"@lewishowles/components": "^1.0.0",
		"@lewishowles/helpers": "^0.19.0",
		"@lewishowles/testing": "^0.9.0",
		"@vueuse/core": "^13.5.0",
		"vue": "^3.5.17"
	},
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@stylistic/eslint-plugin": "^5.2.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/typography": "^0.5.16",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vitest/eslint-plugin": "1.3.4",
		"@vue/test-utils": "^2.4.6",
		"cypress": "^14.5.2",
		"eslint": "^9.31.0",
		"eslint-plugin-cypress": "^5.1.0",
		"eslint-plugin-vue": "^10.3.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.3.0",
		"happy-dom": "^18.0.1",
		"postcss": "^8.5.6",
		"simple-git-hooks": "^2.13.0",
		"start-server-and-test": "^2.0.12",
		"tailwindcss": "^4.1.11",
		"tailwindcss-hocus": "^1.0.0",
		"vite": "^7.0.5",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vitest": "^3.2.4"
	}
}
